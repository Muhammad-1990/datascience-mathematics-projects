   "source": [
    "# Find the max number in a list\n",
    "\n",
    "def find_max(numbers):\n",
    "    max = numbers[0]\n",
    "    for number in numbers:\n",
    "        if number > max:\n",
    "            max = number\n",
    "    return max\n",
    "\n",
    "numbers = [10, 30, 6, 2]\n",
    "max = find_max(numbers)\n",
    "print(max)\n"
   ]
  }
